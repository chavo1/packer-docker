{
   "builders": [{
    "type": "docker",
    "image": "ubuntu",
    "commit": "true"
  }],
  "provisioners":[
    {
      "type": "shell",
      "inline": 
      ["apt-get update",
      "apt-get install -y nginx"
    ]
  }],
  "post-processors": [
    [
      {
        "type": "docker-tag",
        "repository": "chavo/packer",
        "tag": "0.0.2",
        "force":"true"
      },
      {
        "type": "docker-push"
      }
    ]
  ]
}
